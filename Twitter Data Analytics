    "def gg(tweet1):\n",
    "    \n",
    "#     tweet1 = elections['election_texts'][0]\n",
    "\n",
    "    tweet1 = eval(tweet1).decode(\"utf-8\", \"ignore\")\n",
    "    \n",
    "    return tweet1\n"
   ]
  },
  {
   "cell_type": "code",
